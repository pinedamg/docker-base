#Common Commands
alias ls='ls -F --color=always'
alias ll='ls -Glash'
alias l="ll"
alias cp='cp -iv'
alias rm='rm -i'
alias mv='mv -iv'
alias grep='grep --color=auto -in'
alias ..='cd ..'

